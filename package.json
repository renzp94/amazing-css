{
  "name": "amazing-css",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
